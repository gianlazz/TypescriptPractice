{
  "name": "ts_practice",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "copyClientBundle": "ts-node tools/copyClientBundle",
    "buildClient": "npm run --prefix ../client build",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-assets",
    "buildForDistribution": "npm-run-all build buildClient copyClientBundle",
    "dev:distro": "npm-run-all nodemon buildForDistribution start",
    "dev:start": "npm-run-all build start",
    "nodemon": "nodemon --watch src -e ts",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node .",
    "initdb": "ts-node tools/initdb",
    "db:setup": "ts-node ./src/test-utils/setup.ts",
    "test": "npm run clean && npm run db:setup && jest --runInBand"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js",
    "apollo-boost": "./node_modules/apollo-boost/lib/bundle.umd.js",
    "graphql-tag": "./node_modules/graphql-tag/lib/graphql-tag.umd.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@okta/oidc-middleware": "^1.0.2",
    "@types/uuid": "^3.4.4",
    "apollo-boost": "^0.3.1",
    "apollo-server-express": "^2.4.8",
    "axios": "^0.18.0",
    "canvas": "^2.4.1",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-session": "^1.15.6",
    "face-api.js": "^0.19.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "materialize-css": "^1.0.0",
    "pg": "^7.9.0",
    "pg-promise": "^8.6.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.1",
    "type-graphql": "^0.17.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.16",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "youtube-stream-url": "^1.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/express-session": "^1.15.12",
    "@types/fs-extra": "^5.0.5",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.11",
    "@types/materialize-css": "^1.0.6",
    "@types/node": "^11.13.8",
    "@types/pg": "^7.4.14",
    "@types/shelljs": "^0.8.3",
    "@types/vue": "^2.0.0",
    "fs-extra": "^7.0.1",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.14.0",
    "typescript": "^3.4.5"
  }
}
